{
  "name": "@feedback-sdk/shared",
  "version": "1.0.0",
  "description": "Shared code for Feedback SDK server implementations",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./db": {
      "types": "./dist/db/index.d.mts",
      "import": "./dist/db/index.mjs"
    },
    "./services": {
      "types": "./dist/services/index.d.mts",
      "import": "./dist/services/index.mjs"
    },
    "./services/emailService": {
      "types": "./dist/services/emailService.d.mts",
      "import": "./dist/services/emailService.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "mongoose": "^8.9.3",
    "nodemailer": "^6.9.16",
    "@slack/webhook": "^7.0.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/nodemailer": "^6.4.0"
  },
  "author": "",
  "license": "MIT"
}
